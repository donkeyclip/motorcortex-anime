{
  "name": "@donkeyclip/motorcortex-anime",
  "version": "2.1.16",
  "description": "Annime plugin for MotorCortex",
  "main": "dist/motorcortex-anime.cjs.js",
  "module": "dist/motorcortex-anime.esm.js",
  "browser": "dist/motorcortex-anime.umd.js",
  "author": "Donkeyclip (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/@donkeyclip/motorcortex-anime.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "update-force:packages": "./node_modules/npm-check-updates/bin/ncu -u && npm i",
    "update:packages": "npm update --save/--save-dev",
    "lint": "eslint -c .eslintrc src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --config ./demo/webpack.config.js --mode=development --progress ",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test": "HERE GOES YOUR TEST TASK",
    "test:prod": "npm run lint",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "animation"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "mc-animejs-core": "3.1.5"
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": "^7"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.0",
    "@babel/plugin-syntax-jsx": "7.16.0",
    "@babel/plugin-transform-react-jsx": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@donkeyclip/motorcortex": "7.5.5",
    "@donkeyclip/motorcortex-player": "2.3.6",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.1",
    "@semantic-release/npm": "8.0.2",
    "@size-limit/preset-big-lib": "6.0.4",
    "babel-loader": "8.2.3",
    "browserslist": "4.17.5",
    "caniuse-lite": "1.0.30001274",
    "commitizen": "4.2.4",
    "concurrently": "6.3.0",
    "core-js": "3.19.0",
    "css-loader": "6.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.58.3",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "18.0.0",
    "size-limit": "6.0.4",
    "webpack": "5.61.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.4.0"
  }
}
